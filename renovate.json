{
  "extends": [
    "config:base"
  ],
  "ignorePaths": [
    "**/ynab-swagger-client/**"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    },
    {
      "matchPackagePatterns": [
        "^@types"
      ],
      "groupName": "types"
    }
  ],
  "reviewers": [
    "excape"
  ]
}
